<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextCustom" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/custom_edit_text_holo_dark</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="AutoCompleteTextViewCustom" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/custom_list_selector_holo_dark</item>
	  <item name="android:background">@drawable/custom_edit_text_holo_dark</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="CheckBoxCustom" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/custom_btn_check_holo_dark</item>
  </style>

  <style name="RadioButtonCustom" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/custom_btn_radio_holo_dark</item>
  </style>

  <style name="ButtonCustom" parent="android:Widget.Button">
	  <item name="android:background">@drawable/custom_btn_default_holo_dark</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="ImageButtonCustom" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/custom_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerCustom" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/custom_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/custom_list_selector_holo_dark</item>
  </style>

  <style name="SpinnerDropDownItemCustom" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/custom_btn_radio_holo_dark</item>
  </style>
  <style name="TabCustom">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/custom_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextCustom">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarCustom" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/custom_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/custom_progress_indeterminate_horizontal_holo_dark</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="ToggleCustom" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/custom_btn_toggle_holo_dark</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="ListViewCustom" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/custom_list_selector_holo_dark</item>
  </style>

  <style name="ListViewCustom.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/custom_list_selector_holo_dark</item>
  </style>

  <style name="SpinnerItemCustom" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#ffffff</item>
  </style>

</resources>